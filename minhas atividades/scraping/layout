import PySimpleGUI as ps

layout = [
    [ps.Text('Ol√°'), ps.Text('', key='print_nome')],
    [ps.Text('Digite seu nome')],
    [ps.Input(key='nome'),ps.Button('Testar')]
    
]

window = ps.Window('Primeiro Visual', layout)

while True:
    event, value = window.read()
    
    if event == 'Testar':
        window['print_nome'].update (value ['nome'])
        window['teste'].update('Parabens')
        print(value)
        

    if event == ps.WIN_CLOSED:
        break

